{
  "name": "keyword-extractor/keyword-extractor",
  "description": "A package to extract keywords from text",
  "homepage": "https://github.com/iranianpep/keyword-extractor",
  "license": "MIT",
  "authors": [
    {
      "name": "Ehsan Abbasi",
      "email": "ehsan.abb@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "nlp-tools/nlp-tools": "^0.1.3"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.2",
    "phpunit/phpunit": "^9.2"
  },
  "autoload": {
    "psr-4": {
      "KeywordExtractor\\": ["src/KeywordExtractor"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KeywordExtractor\\": "tests/KeywordExtractor"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --no-coverage",
    "test-coverage": "./vendor/bin/phpunit --coverage-html build/coverage"
  }
}
